<beast version='2.0'
       namespace='beast.evolution.alignment:beast.core:beast.evolution.tree.coalescent:beast.core.util:beast.evolution.nuc:beast.evolution.operators:beast.evolution.sitemodel:beast.evolution.substitutionmodel:beast.evolution.likelihood'>


    <data id="alignment" dataType="nucleotide">

        <sequence taxon="ANDI">?</sequence>
        <sequence taxon="DIGI">?</sequence>
        <sequence taxon="KIWI">?</sequence>
        <sequence taxon="CASS">?</sequence>
        <sequence taxon="EMU">?</sequence>
        <sequence taxon="OST">?</sequence>
        <sequence taxon="RHEA">?</sequence>

    </data>


    <tree id='tree' spec='beast.util.TreeParser' IsLabelledNewick='true'
          newick='((((ANDI:0.030087877798038076,DIGI:0.030087877798038076):0.07826299311160571,KIWI:0.10835087090964379):0.015048086508534522,(CASS:0.07249889947902681,EMU:0.07249889947902681):0.050900057939151505):0.009462130815896627,(OST:0.11832057572512639,RHEA:0.11832057572512639):0.014540512508948555)' taxa='@alignment'/>

    <run spec="beast.app.seqgen.SequenceSimulator" id="seqgen" data='@alignment' tree='@tree' sequencelength='5000'>
        <siteModel spec='SiteModel' id="siteModel" gammaCategoryCount='1'>
        <substModel spec='HKY' id='hky'>
            <parameter name='kappa' id="hky.kappa" value="5.0"/>
            <frequencies id='freqs' spec='Frequencies' frequencies="0.25 0.25 0.25 0.25"/>
        </substModel>
        </siteModel>
        <branchRateModel id="UCRelaxedClock" spec="beast.evolution.branchratemodel.UCRelaxedClockModel" tree="@tree">
            <parameter id="ucldMean" estimate="false" name="clock.rate">1.0</parameter>
            <!--
<parameter id="branchrates" name="rates" spec="parameter.RealParameter" dimension="12">1</parameter>
-->
            <parameter id="branchrates" name="rates" spec="parameter.RealParameter" value="0.9 0.8 1 1 1 1 1 1.1 0.9 0.8 0.7 1.2"/>
            <distr id='lognormal' spec="beast.math.distributions.LogNormalDistributionModel" meanInRealSpace="true">
                <parameter name='M' id='M' value="1.0"/>
                <parameter name='S' id='ucld.stdev' value="0.2"/>
            </distr>
        </branchRateModel>

    </run>
</beast>



