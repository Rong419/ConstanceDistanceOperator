<?xml version="1.0" encoding="UTF-8" standalone="no"?><beast beautitemplate='Standard' beautistatus='' namespace="consoperators:beast.core:beast.evolution.alignment:beast.evolution.tree.coalescent:beast.core.util:beast.evolution.nuc:beast.evolution.operators:beast.evolution.sitemodel:beast.evolution.substitutionmodel:beast.evolution.likelihood" required="" version="2.4">


    <data
id="dna"
name="alignment">
        <sequence id="seq_Human" taxon="Human" totalcount="4" value="?"/>
        <sequence id="seq_Seal" taxon="Seal" totalcount="4" value="?"/>
        <sequence id="seq_Frog" taxon="Frog" totalcount="4" value="?"/>

        <sequence id="seq_Whale" taxon="Whale" totalcount="4" value="?"/>

    </data>

<map name="Uniform" >beast.math.distributions.Uniform</map>
<map name="Exponential" >beast.math.distributions.Exponential</map>
<map name="LogNormal" >beast.math.distributions.LogNormalDistributionModel</map>
<map name="Normal" >beast.math.distributions.Normal</map>
<map name="Beta" >beast.math.distributions.Beta</map>
<map name="Gamma" >beast.math.distributions.Gamma</map>
<map name="LaplaceDistribution" >beast.math.distributions.LaplaceDistribution</map>
<map name="prior" >beast.math.distributions.Prior</map>
<map name="InverseGamma" >beast.math.distributions.InverseGamma</map>
<map name="OneOnX" >beast.math.distributions.OneOnX</map>



    <run id="mcmc" spec="MCMC" chainLength="10000000" sampleFromPrior="true">
         <state id="state" storeEvery="5000">
<!--
         <tree spec="beast.util.TreeParser" id="Tree.t:dna" name="stateNode" IsLabelledNewick="true"
               newick="((Human:4,Seal:4):4,Frog:8)" taxa="@dna"/>
-->

             <tree spec="beast.util.TreeParser" id="Tree.t:dna" name="stateNode" IsLabelledNewick="true"
               newick="(((Human:2,Seal:2):2,Whale:4):4,Frog:8)" taxa="@dna"/>

<!--
         <parameter id="rates.c:dna" name="stateNode" value="0.75 0.25 1.0 2.25"/>
-->

         <parameter id="rates.c:dna" name="stateNode" value="0.5 0.3 0.2 0.1 0.2 0.25"/>

    </state>

    <distribution id="posterior" spec="util.CompoundDistribution">
        <distribution id="prior" spec="util.CompoundDistribution">

            <distribution id="CoalescentConstant.t:dna" spec="Coalescent">
                <populationModel id="ConstantPopulation.t:dna" spec="ConstantPopulation" popSize="0.3"/>
                <treeIntervals id="TreeIntervals.t:dna" spec="TreeIntervals" tree="@Tree.t:dna"/>
            </distribution>

         <prior id="RatePriorDistrbution.t:dna" name="distribution" x="@rates.c:dna">
         <LogNormal id="LogNormalDistribution.c:dna" M="-0.03" S="0.25" meanInRealSpace="false" name="distr"/>
         </prior>

            <distribution id='mrcaPrior1' spec='beast.math.distributions.MRCAPrior' tree='@Tree.t:dna'>
            <taxonset spec='TaxonSet' id='ingroup1'>
                <taxon spec='Taxon' id='Human'/>
                <taxon spec='Taxon' id='Seal'/>

                <taxon spec='Taxon' id='Whale'/>

            </taxonset>
        </distribution>


        </distribution>

        <distribution id="likelihood" spec="util.CompoundDistribution" useThreads="true">
            <distribution id="treeLikelihood.dna" spec="ThreadedTreeLikelihood" data="@dna" tree="@Tree.t:dna">
                <siteModel id="SiteModel.s:dna" spec="SiteModel" gammaCategoryCount="1" mutationRate="1.0" shape="1.0">
                    <parameter id="proportionInvariant.s:dna" estimate="false" lower="0.0" name="proportionInvariant" upper="1.0">0.0</parameter>
                    <substModel id="JC69.s:dna" spec="JukesCantor"/>
                </siteModel>
                <branchRateModel id="RelaxedClock.c:dna" spec="beast.evolution.branchratemodel.UCRelaxedClockModel"
                         rates="@rates.c:dna" tree="@Tree.t:dna" distr="@LogNormalDistribution.c:dna">
        </branchRateModel>
            </distribution>
        </distribution>
    </distribution>


    <operator id="TwoNodeOperator.c:dna" spec="TwoNodeOperator" branchRateModel="@RelaxedClock.c:dna"
              tree="@Tree.t:dna" rates="@rates.c:dna" twindowSize="2" weight="20"/>

<!--
    <operator id="TwoNodeOperator.c:dna" spec="BranchOperator" branchRateModel="@RelaxedClock.c:dna"
                  tree="@Tree.t:dna" rates="@rates.c:dna" twindowSize="2" weight="20"/>
-->
<!--
    <operator id="InternalnodesOperator.c:dna" spec="InConstantDistanceOperator" branchRateModel="@RelaxedClock.c:dna" twindowSize="1"
          tree="@Tree.t:dna" rates="@rates.c:dna"  weight="20"/>


    <operator id="RootOperator1.c:dna" spec="SimpleDistance" branchRateModel="@RelaxedClock.c:dna"
                  twindowSize="2" tree="@Tree.t:dna" rates="@rates.c:dna"  weight="20"/>
-->
<!--
    <operator id="RootOperator2.c:dna" spec="SmallPulleyV1" branchRateModel="@RelaxedClock.c:dna"
                  dwindowSize="2" tree="@Tree.t:dna" rates="@rates.c:dna"  weight="10"/>
-->
<!--
    <operator id="TreeScaler.t:dna" spec="ScaleOperator" scaleFactor="0.5" tree="@Tree.t:dna" weight="3.0"/>

    <operator id="TreeRootScaler.t:dna" spec="ScaleOperator" rootOnly="true" scaleFactor="0.5" tree="@Tree.t:dna" weight="3.0"/>

    <operator id="ratesRandomWalk.c:dna" spec="RealRandomWalkOperator" parameter="@rates.c:dna" weight="10.0" windowSize="3"/>

-->
<!--
    <parameter id="ucldMean.c:dna" estimate="false" name="clock.rate">1.0</parameter>

    <operator id="YuleBirthRateScaler.t:dna" spec="ScaleOperator" parameter="@birthRate.t:dna" scaleFactor="0.75" weight="3.0" />

    <operator id="YuleModelUniformOperator.t:dna" spec="Uniform" tree="@Tree.t:dna" weight=""/>

    <operator id="YuleModelSubtreeSlide.t:dna" spec="SubtreeSlide" tree="@Tree.t:dna" weight="15.0"/>

    <operator id="YuleModelNarrow.t:dna" spec="Exchange" tree="@Tree.t:dna" weight="15.0"/>

    <operator id="YuleModelWide.t:dna" spec="Exchange" isNarrow="false" tree="@Tree.t:dna" weight="3.0"/>

    <operator id="YuleModelWilsonBalding.t:dna" spec="WilsonBalding" tree="@Tree.t:dna" weight="3.0"/>

    <operator id="FixMeanMutationRatesOperator" spec="DeltaExchangeOperator" delta="0.75" weight="2.0">
    <parameter idref="mutationRate.s:dna"/>
    <weightvector id="weightparameter" spec="parameter.IntegerParameter" estimate="false" lower="0" upper="0">705</weightvector>
    </operator>

    <operator id="gammaShapeScaler.s:dna" spec="ScaleOperator" parameter="@gammaShape.s:dna" scaleFactor="0.5" weight="0.5"/>

    <operator id="ucldStdevScaler.c:dna" spec="ScaleOperator" parameter="@ucldStdev.c:dna" scaleFactor5" weight="3.0"/>
-->

    <logger id="tracelog" fileName="test.log" logEvery="2000" model="@posterior" sanitiseHeaders="true" sort="smart">
        <log idref="posterior"/>
        <log idref="likelihood"/>
        <log idref="prior"/>
        <log idref="treeLikelihood.dna"/>
        <log id="TreeLength.t:ratites" spec="beast.evolution.tree.TreeStatLogger" tree="@Tree.t:dna"/>
        <log id="rate.c:dna" spec="beast.evolution.branchratemodel.RateStatistic" branchratemodel="@RelaxedClock.c:dna" tree="@Tree.t:dna"/>
        <log idref="CoalescentConstant.t:dna"/>
        <log idref="RatePriorDistrbution.t:dna"/>
        <log idref="rates.c:dna"/>

        <log idref="mrcaPrior1"/>

    </logger>

    <logger id="screenlog" logEvery="100000">
        <log idref="posterior"/>
        <log id="ESS.0" spec="util.ESS" arg="@posterior"/>
        <log idref="likelihood"/>
        <log idref="prior"/>
    </logger>

    <logger id="treelog.t:dna" fileName="test.trees" logEvery="2000" mode="tree">
        <log id="TreeWithMetaDataLogger.t:dna" spec="beast.evolution.tree.TreeWithMetaDataLogger" branchratemodel="@RelaxedClock.c:dna" tree="@Tree.t:dna"/>
    </logger>

</run>

</beast>
