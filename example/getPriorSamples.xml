    <state id="state" storeEvery="5000">
        <tree id="Tree.t:ratites" name="stateNode">
            <taxonset id="TaxonSet.ratites" spec="TaxonSet">
                <alignment idref="ratites"/>
            </taxonset>
        </tree>
        <parameter id="ucldStdev.c:ratites" lower="0.0" name="stateNode">0.1</parameter>
        <stateNode id="rates.c:ratites" spec="parameter.RealParameter" dimension="38">1.0</stateNode>
        <parameter id="BirthRate.t:ratites" name="stateNode">2.0</parameter>
        <parameter id="kappa.t:ratites" lower="0.0" name="stateNode">5.0</parameter>
        <parameter id="freqParameter.t:ratites" dimension="4" name="stateNode">0.25</parameter>
    </state>


    <branchRateModel id="RelaxedClock.c:ratites" spec="beast.evolution.branchratemodel.UCRelaxedClockModel"
                     rates="@rates.c:ratites" tree="@Tree.t:ratites" distr="@LogNormalDistribution2">
        <parameter id="ucldMean.c:ratites" estimate="false" name="clock.rate">1.0</parameter>
    </branchRateModel>


    <run spec="DirectSimulator" nSamples="1000000">
        <distribution id="prior" spec="util.CompoundDistribution">

            <distribution id="YuleModel.t:ratites" spec="beast.evolution.speciation.YuleModel" birthDiffRate="@BirthRate.t:ratites" tree="@Tree.t:ratites"/>

            <prior id="BirthRatePrior.t:ratites" name="distribution" x="@BirthRate.t:ratites">
                <LogNormal id="LogNormalDistribution1" S="1"  M="2" meanInRealSpace="true" name="distr"/>
            </prior>

            <prior id="FrequencyPrior.t:ratites" name="distribution" x="@freqParameter.t:ratites">
                <Dirichlet id="Dirichlet1" alpha="10 10 10 10" name="distr"/>
            </prior>

            <prior id="KappaPrior.t:ratites" name="distribution" x="@kappa.t:ratites">
                <LogNormal id="LogNormalDistribution0" S="1.25"  M="5.0" meanInRealSpace="true"  name="distr"/>
            </prior>

            <prior id="RatePrior.t:ratites" name="distribution" x="@rates.c:ratites">
                <LogNormal id="LogNormalDistribution2" S="@ucldStdev.c:ratites"  M="1" meanInRealSpace="true" name="distr"/>
            </prior>

            <prior id="ucldStdevPrior.c:ratites" name="distribution" x="@ucldStdev.c:ratites">
                <LogNormal id="LogNormalDistribution3" S="0.5"  M="0.1" meanInRealSpace="true" name="distr"/>
            </prior>
        </distribution>

        <logger id="tracelog" fileName="ratites.log" logEvery="1000" model="@prior" sanitiseHeaders="true" sort="smart">
            <log idref="BirthRate.t:ratites"/>
            <log idref="kappa.t:ratites"/>
            <log idref="freqParameter.t:ratites"/>
            <log idref="ucldStdev.c:ratites"/>
            <log id="rate.c:ratites" spec="beast.evolution.branchratemodel.RateStatistic" branchratemodel="@RelaxedClock.c:ratites" tree="@Tree.t:ratites"/>
            <log id="TreeStat.t:ratites" spec="beast.evolution.tree.TreeStatLogger" tree="@Tree.t:ratites"/>
            <log idref="rates.c:ratites"/>
        </logger>

        <logger id="treelog.t:ratites" fileName="$(tree).trees" logEvery="1000" mode="tree">
            <log idref="Tree.t:ratites"/>
        </logger>

    </run>

</beast>
