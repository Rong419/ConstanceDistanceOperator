<?xml version="1.0" encoding="UTF-8" standalone="no"?><beast beautitemplate='Standard' beautistatus='' namespace="beast.core:beast.evolution.alignment:beast.evolution.tree.coalescent:beast.core.util:beast.evolution.nuc:beast.evolution.operators:beast.evolution.sitemodel:beast.evolution.substitutionmodel:beast.evolution.likelihood" required="" version="2.6">


    <data id="dna" spec="Alignment" name="alignment">
        <sequence id="seq_Carp" spec="Sequence" taxon="Carp" totalcount="4" value=""/>
        <sequence id="seq_Chicken" spec="Sequence" taxon="Chicken" totalcount="4" value="ATGGCCAACCACTCCCAACTAGGCTTTCAAGACGCCTCATCCCCCATCATAGAAGAGCTCGTTGAATTCCACGACCACGCCCTGATAGTCGCACTAGCAATTTGCAGCTTAGTACTCTACCTTCTAACTCTTATACTTATAGAAAAACTATCA---------------------CTGTCATCTTAA"/>
        <sequence id="seq_Cow" spec="Sequence" taxon="Cow" totalcount="4" value="---------------------TAA"/>
        <sequence id="seq_Frog" spec="Sequence" taxon="Frog" totalcount="4" value="---GAAGCATCACTA------AGA"/>
        <sequence id="seq_Human" spec="Sequence" taxon="Human" totalcount="4" value="---------------------GGGCCCGTATTTACCCTATAG"/>
        <sequence id="seq_Loach" spec="Sequence" taxon="Loach" totalcount="4" value=""/>
        <sequence id="seq_Mouse" spec="Sequence" taxon="Mouse" totalcount="4" value="---------------------TAA"/>
        <sequence id="seq_Rat" spec="Sequence" taxon="Rat" totalcount="4" value="---------------------TAA"/>
        <sequence id="seq_Seal" spec="Sequence" taxon="Seal" totalcount="4" value="---------------------TAA"/>
        <sequence id="seq_Whale" spec="Sequence" taxon="Whale" totalcount="4" value="---------------------TAA"/>
    </data>

<map name="Uniform" >beast.math.distributions.Uniform</map>
<map name="Exponential" >beast.math.distributions.Exponential</map>
<map name="LogNormal" >beast.math.distributions.LogNormalDistributionModel</map>
<map name="Normal" >beast.math.distributions.Normal</map>
<map name="Beta" >beast.math.distributions.Beta</map>
<map name="Gamma" >beast.math.distributions.Gamma</map>
<map name="LaplaceDistribution" >beast.math.distributions.LaplaceDistribution</map>
<map name="prior" >beast.math.distributions.Prior</map>
<map name="InverseGamma" >beast.math.distributions.InverseGamma</map>
<map name="OneOnX" >beast.math.distributions.OneOnX</map>


<run id="mcmc" spec="MCMC" chainLength="2000000" sampleFromPrior="true">
    <state id="state" spec="State" storeEvery="5000">
        <tree id="Tree.t:dna" spec="beast.evolution.tree.Tree" name="stateNode">
            <taxonset id ="TaxonSet.dna" spec="TaxonSet">
            <alignment idref="dna"/>
            </taxonset>
        </tree>
        <parameter id="kappa.s:dna" spec="parameter.RealParameter" lower="0.0" name="stateNode">2.0</parameter>
        <parameter id="fastUcldMean.c:dna" spec="parameter.RealParameter" name="stateNode">1.0</parameter>
        <parameter id="fastUcldStdev.c:dna" spec="parameter.RealParameter" lower="0.0" name="stateNode">0.001</parameter>
        <parameter id="fastRates.c:dna" spec="parameter.RealParameter" lower="0.0" name="stateNode">0.5</parameter>
        <parameter id="BDBirthRate.t:dna" spec="parameter.RealParameter" lower="0.0" name="stateNode" upper="10000.0">1.0</parameter>
        <parameter id="BDDeathRate.t:dna" spec="parameter.RealParameter" lower="0.0" name="stateNode" upper="1.0">0.5</parameter>
        <parameter id="freqParameter.s:dna" spec="parameter.RealParameter" dimension="4" lower="0.0" name="stateNode" upper="1.0">0.25</parameter>
    </state>

    <init id="RandomTree.t:dna" spec="beast.evolution.tree.RandomTree" estimate="false" initial="@Tree.t:dna" taxa="@dna">
        <populationModel id="ConstantPopulation0.t:dna" spec="ConstantPopulation">
            <parameter id="randomPopSize.t:dna" spec="parameter.RealParameter" name="popSize">1.0</parameter>
        </populationModel>
    </init>

    <distribution id="posterior" spec="util.CompoundDistribution">
        <distribution id="prior" spec="util.CompoundDistribution">
            <distribution id="BirthDeath.t:dna" spec="beast.evolution.speciation.BirthDeathGernhard08Model" birthDiffRate="@BDBirthRate.t:dna" relativeDeathRate="@BDDeathRate.t:dna" tree="@Tree.t:dna"/>

            <prior id="BirthRatePrior.t:dna" name="distribution" x="@BDBirthRate.t:dna">
                <Uniform id="Uniform.4" name="distr" upper="1000.0"/>
            </prior>

            <prior id="DeathRatePrior.t:dna" name="distribution" x="@BDDeathRate.t:dna">
                <Uniform id="Uniform.5" name="distr"/>
            </prior>

            <prior id="FastRatePriorDistrbution.c:dna" name="distribution" x="@fastRates.c:dna">
                <LogNormal id="LogNormalDistributionModel" S="@fastUcldStdev.c:dna" meanInRealSpace="true" name="distr">
                    <parameter id="RealParameter" spec="parameter.RealParameter" estimate="false" name="M">1.0</parameter>
                </LogNormal>
            </prior>

            <prior id="fastUcldStdevPrior.c:dna" name="distribution" x="@fastUcldStdev.c:dna">
<!--
                <LogNormal id="LogNormalDistributionModel1" name="distr" meanInRealSpace="true">
                    <parameter id="RealParameter1" spec="parameter.RealParameter" estimate="false" name="M">0.2</parameter>
                    <parameter id="RealParameter2" spec="parameter.RealParameter" estimate="false" name="S">2.0</parameter>
                </LogNormal>
-->

                <Exponential id="Uniform.10" name="distr" mean="0.1"/>

                <!--
                <Gamma id="Gamma.10" name="distr">
                    <parameter id="RealParameter.47" spec="parameter.RealParameter" estimate="false" name="alpha">0.5396</parameter>
                    <parameter id="RealParameter.48" spec="parameter.RealParameter" estimate="false" name="beta">0.3819</parameter>
                </Gamma>
-->
            </prior>

            <prior id="fastUcldMeanPrior.c:Dengue4" name="distribution" x="@fastUcldMean.c:dna">
                <!--<Normal id="Gamma.2" name="distr" mean="1.0" sigma="1"/>-->
                <LogNormal id="Gamma.2" name="distr" M="1" S="0.5" meanInRealSpace="true"/>
            </prior>

            <prior id="FrequenciesPrior.s:dna" name="distribution" x="@freqParameter.s:dna">
                <Uniform id="Uniform.3" name="distr"/>
            </prior>

            <prior id="KappaPrior.s:dna" name="distribution" x="@kappa.s:dna">
                <LogNormal id="LogNormalDistributionModel.0" name="distr">
                    <parameter id="RealParameter.1" spec="parameter.RealParameter" estimate="false" name="M">1.0</parameter>
                    <parameter id="RealParameter.2" spec="parameter.RealParameter" estimate="false" name="S">1.25</parameter>
                </LogNormal>
            </prior>
        </distribution>
        <distribution id="likelihood" spec="util.CompoundDistribution" useThreads="true">
            <distribution id="treeLikelihood.dna" spec="ThreadedTreeLikelihood" data="@dna" tree="@Tree.t:dna">
                <siteModel id="SiteModel.s:dna" spec="SiteModel">
                    <parameter id="mutationRate.s:dna" spec="parameter.RealParameter" estimate="false" name="mutationRate">1.0</parameter>
                    <parameter id="gammaShape.s:dna" spec="parameter.RealParameter" estimate="false" name="shape">1.0</parameter>
                    <parameter id="proportionInvariant.s:dna" spec="parameter.RealParameter" estimate="false" lower="0.0" name="proportionInvariant" upper="1.0">0.0</parameter>
                    <substModel id="hky.s:dna" spec="HKY" kappa="@kappa.s:dna">
                        <frequencies id="estimatedFreqs.s:dna" spec="Frequencies" frequencies="@freqParameter.s:dna"/>
                    </substModel>
                </siteModel>
                <branchRateModel id="FastRelaxedClock.c:dna" spec="beast.evolution.branchratemodel.UCRelaxedClockModel" clock.rate="@fastUcldMean.c:dna" rates="@fastRates.c:dna" tree="@Tree.t:dna">
                    <LogNormal id="FastLogNormalDistributionModel.c:dna" S="@fastUcldStdev.c:dna" meanInRealSpace="true" name="distr">
                        <parameter id="RealParameter3" spec="parameter.RealParameter" estimate="false" name="M">1.0</parameter>
                    </LogNormal>
                </branchRateModel>
            </distribution>
        </distribution>
    </distribution>
<!--
    <operator id="InternalnodesOperator.c:dna" spec="consoperators.InConstantDistanceOperator"
              twindowSize="0.02"  tree="@Tree.t:dna" rates="@fastRates.c:dna"  weight="5"/>

    <operator id="RootOperator1.c:dna" spec="consoperators.SimpleDistance"
              twindowSize="0.02" tree="@Tree.t:dna" rates="@fastRates.c:dna"  weight="2"/>

    <operator id="RootOperator2.c:dna" spec="consoperators.SmallPulley"
              dwindowSize="0.02" tree="@Tree.t:dna" rates="@fastRates.c:dna"  weight="2"/>

    <operator id="RootOperator3.c:dna" spec="consoperators.BigPulley"
              twindowSize="0.02" dwindowSize="0.032" tree="@Tree.t:dna" rates="@fastRates.c:dna"  weight="1"/>
-->

    <operator id="FastUcldStdevScaler.c:dna" spec="consoperators.UcldScalerOperator"
              rates="@fastRates.c:dna" stdev="@fastUcldStdev.c:dna" distr="@FastLogNormalDistributionModel.c:dna" scaleFactor="0.8" weight="10.0"/>

<!--
    <operator id="FastRatesInternal.c:dna" spec="consoperators.InternalOperator"
              rateQuantiles="@fastRates.c:dna" twindowSize="0.1" tree="@Tree.t:dna" distr="@FastLogNormalDistributionModel.c:dna" weight="20.0"/>
-->
    <operator id="RatesRandomWalk.c:dna" spec="RealRandomWalkOperator" parameter="@fastRates.c:dna" weight="3.0" windowSize="0.1"/>

    <operator id="RatesScaleOperator.c:dna" spec="ScaleOperator" scaleFactor="0.5" parameter="@fastRates.c:dna" weight="3.0"/>
<!--
    <operator id="fastUcldMeanScaler.c:dna" spec="ScaleOperator" parameter="@fastUcldMean.c:dna" scaleFactor="0.5" weight="5.0"/>
-->
<!--
    <operator id="fastUcldStdevScaler.c:dna4" spec="ScaleOperator" parameter="@fastUcldStdev.c:dna" scaleFactor="0.5" weight="5.0"/>

    <operator id="BirthRateScaler.t:dna" spec="ScaleOperator" parameter="@BDBirthRate.t:dna" scaleFactor="0.75" weight="3.0"/>

    <operator id="DeathRateScaler.t:dna" spec="ScaleOperator" parameter="@BDDeathRate.t:dna" scaleFactor="0.75" weight="3.0"/>

    <operator id="BirthDeathTreeScaler.t:dna" spec="ScaleOperator" scaleFactor="0.5" tree="@Tree.t:dna" weight="3.0"/>

    <operator id="BirthDeathTreeRootScaler.t:dna" spec="ScaleOperator" rootOnly="true" scaleFactor="0.5" tree="@Tree.t:dna" weight="3.0"/>

    <operator id="KappaScaler.s:dna" spec="ScaleOperator" parameter="@kappa.s:dna" scaleFactor="0.5" weight="0.5"/>

    <operator id="BirthDeathUniformOperator.t:dna" spec="Uniform" tree="@Tree.t:dna" weight="30.0"/>

    <operator id="BirthDeathSubtreeSlide.t:dna" spec="SubtreeSlide" tree="@Tree.t:dna" weight="15.0"/>

    <operator id="BirthDeathNarrow.t:dna" spec="Exchange" tree="@Tree.t:dna" weight="15.0"/>

    <operator id="BirthDeathWide.t:dna" spec="Exchange" isNarrow="false" tree="@Tree.t:dna" weight="3.0"/>

    <operator id="BirthDeathWilsonBalding.t:dna" spec="WilsonBalding" tree="@Tree.t:dna" weight="3.0"/>


    <operator id="FrequenciesExchanger.s:dna" spec="DeltaExchangeOperator" delta="0.01" weight="0.5">
        <parameter idref="freqParameter.s:dna"/>
    </operator>
-->
    <logger id="tracelog" spec="Logger" fileName="dna.log" logEvery="1" model="@posterior" sanitiseHeaders="true" sort="smart">
        <log idref="posterior"/>
        <log idref="likelihood"/>
        <log idref="prior"/>
        <log idref="treeLikelihood.dna"/>
        <log id="TreeHeight.t:dna" spec="beast.evolution.tree.TreeHeightLogger" tree="@Tree.t:dna"/>
        <log idref="kappa.s:dna"/>
        <log idref="fastUcldMean.c:dna"/>
        <log idref="fastUcldStdev.c:dna"/>
        <log idref="BirthDeath.t:dna"/>
        <log idref="BDBirthRate.t:dna"/>
        <log idref="BDDeathRate.t:dna"/>
        <log idref="freqParameter.s:dna"/>
        <log idref="fastRates.c:dna"/>
    </logger>

    <logger id="screenlog" spec="Logger" logEvery="10000">
        <log idref="posterior"/>
        <log idref="likelihood"/>
        <log idref="prior"/>
    </logger>

    <logger id="treelog.t:dna" spec="Logger" fileName="$(tree).trees" logEvery="1000" mode="tree">
        <log id="TreeWithMetaDataLogger.t:dna" spec="beast.evolution.tree.TreeWithMetaDataLogger" branchratemodel="@FastRelaxedClock.c:dna" tree="@Tree.t:dna"/>
    </logger>

</run>

</beast>
